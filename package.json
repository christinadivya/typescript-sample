{
  "name": "typescript-auth",
  "version": "0.0.1",
  "description": "Webservices for typescript-auth",
  "main": "dist/index.js",
  "private": true,
  "engines": {
    "node": ">=16.16.0",
    "npm": ">=8.15.1"
  },
  "scripts": {
    "lint:fix": "eslint --color --quiet --fix --ext .ts,.js .",
    "dev:lint:fix": "nodemon -e ts -x npm run lint:fix",
    "dev:prettier": "prettier --write .",
    "dev:tsc": "tsc",
    "clean": "rimraf dist/",
    "copy-files": "copyfiles -u 1 src/**/*.html src/**/*.yml src/**/*.json dist/",
    "build": "npm run clean && tsc && npm run copy-files",
    "copy-env": "cp ./local-setup/local-env .env",
    "start": "node dist/index.js",
    "start:lint": "eslint --color --quiet --ext .ts,.js .",
    "start:local": "nodemon -w src -w index.ts -e ts,js,json,yml -x babel-node --require dotenv/config --extensions .ts,.js src/index.ts",
    "dev": "npm run copy-env && npm-run-all -p start:local dev:tsc dev:lint:fix dev:prettier",
    "typeorm": "typeorm-ts-node-commonjs",
    "typeorm_src": "typeorm-ts-node-commonjs -d src/data-source.ts",
    "migration": "npm run typeorm_src migration:run",
    "seed": "ts-node ./node_modules/typeorm-extension/dist/cli/index.js seed",
    "local": "npm run copy-env && docker-compose -f ./local-setup/docker-compose.yml up -d",
    "test:unit": "NODE_ENV=test mocha --exit tests/**/*.spec.ts --timeout 8000",
    "test:unit:coverage": "nyc --reporter=lcov --report-dir=coverage/unit npm run test:unit",
    "test": "npm run test:unit:coverage",
    "test:integration:start-server": "NODE_ENV=local nyc --reporter=lcov --report-dir=coverage/integration ts-node src/index.ts",
    "test:integration:smoke": "NODE_ENV=local mocha --exit --timeout 5000 integration-tests/**/*smoke*.ts",
    "test:integration:run": "wait-on http://localhost:8000/status && npm run test:integration:smoke",
    "test:integration": "npm-run-all --race -p test:integration:start-server test:integration:run",
    "sonar": "sonar-scanner",
    "postinstall": "patch-package"
  },
  "keywords": [
    "express",
    "node",
    "mysql",
    "REST",
    "API",
    "docker",
    "swagger",
    "eslint"
  ],
  "dependencies": {
    "@promster/express": "^7.0.5",
    "@sentry/node": "^7.15.0",
    "@sentry/tracing": "^7.15.0",
    "axios": "^1.2.1",
    "bcrypt": "^5.0.1",
    "chalk": "^4.1.2",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-basic-auth": "^1.2.1",
    "express-validation": "^4.1.0",
    "helmet": "^6.0.0",
    "i18next-fs-backend": "^1.1.5",
    "i18next-http-middleware": "^3.2.1",
    "ioredis": "^5.2.3",
    "joi-password-complexity": "^5.1.0",
    "jsonwebtoken": "^8.5.1",
    "limiter": "^2.1.0",
    "log4js": "^6.7.0",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.3",
    "nyc": "^15.1.0",
    "patch-package": "^6.4.7",
    "prom-client": "^14.1.0",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.5.0",
    "typeorm": "0.3.10",
    "typeorm-extension": "^2.1.15",
    "uuidv4": "^6.2.13",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.3",
    "@babel/node": "^7.19.1",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.19.3",
    "@babel/plugin-transform-runtime": "^7.19.1",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/register": "^7.18.9",
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/chai-as-promised": "^7.1.5",
    "@types/cors": "^2.8.12",
    "@types/crypto-js": "^4.1.1",
    "@types/express": "^4.17.14",
    "@types/i18n": "^0.13.4",
    "@types/i18next-fs-backend": "^1.1.2",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/mocha": "^10.0.0",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.8.4",
    "@types/redis": "2.8",
    "@types/sinon": "^10.0.13",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/yamljs": "^0.2.31",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "chai-exclude": "^2.1.0",
    "chakram": "^1.5.0",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.25.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-tsdoc": "^0.2.17",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.1.1",
    "rimraf": "^3.0.2",
    "sinon": "^14.0.1",
    "sonar-scanner": "^3.1.0",
    "supertest": "^6.3.1",
    "ts-node": "10.9.1",
    "typescript": "4.8.4",
    "wait-on": "^6.0.1"
  }
}
